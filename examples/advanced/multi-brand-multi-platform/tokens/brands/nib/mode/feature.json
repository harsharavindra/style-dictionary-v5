        "error": { "$value": "{primitive.color.functional.error.500}" },
        "warning": { "$value": "{primitive.color.functional.warning.750}" },
        "info": { "$value": "{primitive.color.functional.info.750}" }
      },
      "fg": {
        "default": { "$value": "{primitive.color.neutral.950}" },
        "brand": { "$value": "{primitive.color.primary.830}" },
        "label": { "$value": "{primitive.color.primary.830}" },
        "link": {
          "default": { "$value": "{primitive.color.primary.830}" },
