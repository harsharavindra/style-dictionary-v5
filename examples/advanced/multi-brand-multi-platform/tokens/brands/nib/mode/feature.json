      "border": {
        "default": { "$value": "{primitive.color.sage.835}" }
      }
    }
  },
  "logo": {
    "meta": {
      "color": {
        "default": { "$value": "brightGreen" }
      }
    }
  },
  "productCard": {
    "color": {
      "bg": {
        "highlight": { "$value": "{primitive.color.primary.200}" }
      },
      "fg": {
        "highlight": { "$value": "{primitive.color.primary.830}" }
      }
    }
  },
  "progressStepper": {
    "color": {
