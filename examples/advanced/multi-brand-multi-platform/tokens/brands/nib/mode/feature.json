        "hover": { "$value": "{theme.color.fg.prominent}" }
      }
    }
  },
  "drawer": {
    "color": {
      "bg": {
        "default": { "$value": "{primitive.color.primary.200}" },
        "top": { "$value": "{primitive.color.primary.275}" }
