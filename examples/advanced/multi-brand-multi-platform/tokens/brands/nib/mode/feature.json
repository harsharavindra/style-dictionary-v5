        "gentle": { "$value": "{primitive.color.neutral.150}" },
        "muted": { "$value": "{primitive.color.neutral.150}" },
        "error": { "$value": "{primitive.color.neutral.150}" },
        "success": { "$value": "{primitive.color.neutral.150}" },
        "info": { "$value": "{primitive.color.neutral.150}" }
      }
    }
  },
  "tooltip": {
    "color": {
      "bg": { "$value": "{primitive.color.primary.50}" }
    }
  }
}
