      "fg": {
        "top": { "$value": "{primitive.color.primary.830}" },
        "menu": { "$value": "{primitive.color.primary.830}" },
        "item": { "$value": "{theme.color.fg.default}" },
        "chevron": { "$value": "{theme.color.fg.muted}" },
        "close": { "$value": "{theme.color.fg.muted}" },
        "link": {
          "default": { "$value": "{theme.color.fg.default}" },
          "hover": { "$value": "{theme.color.fg.link.hover}" },
          "pressed": { "$value": "{theme.color.fg.link.active}" }
        }
      },
      "border": {
        "divider": { "$value": "{primitive.color.primary.275}" }
      }
    }
