        "error": { "$value": "{primitive.color.functional.error.750}" },
        "warning": { "$value": "{primitive.color.functional.warning.750}" },
        "info": { "$value": "{primitive.color.sage.835}" }
      }
    },
    "meta": {
      "modeName": { "$value": "feature" },
      "isDark": { "$value": true }
    }
  },
  "alert": {
    "color": {
      "bg": {
        "success": { "$value": "{primitive.color.primary.150}" },
        "error": { "$value": "{primitive.color.functional.error.150}" },
        "warning": { "$value": "{primitive.color.functional.warning.150}" },
        "info": { "$value": "{primitive.color.functional.info.100}" }
      }
    }
  },
  "button": {
    "color": {
      "bg": {
        "primary": {
          "disabled": {
            "$value": "{button.color.bg.primary.default}",
            "alpha": 0.4
          }
        }
      },
      "fg": {
        "primary": {
          "disabled": { "$value": "{primitive.color.primary.275}" }
        },
        "secondary": {
          "default": { "$value": "{theme.color.fg.link.default}" },
          "hover": { "$value": "{theme.color.fg.link.hover}" },
          "active": { "$value": "{theme.color.fg.link.active}" },
          "disabled": {
            "$value": "{button.color.fg.secondary.default}",
            "alpha": 0.4
          }
        },
        "tertiary": {
          "default": { "$value": "{theme.color.fg.link.default}" },
          "hover": { "$value": "{theme.color.fg.link.hover}" },
          "active": { "$value": "{theme.color.fg.link.active}" },
          "disabled": {
            "$value": "{button.color.fg.tertiary.default}",
            "alpha": 0.4
          }
        },
        "destructive": {
          "default": { "$value": "{primitive.color.functional.error.750}" },
          "hover": { "$value": "{primitive.color.functional.error.900}" },
          "active": { "$value": "{primitive.color.functional.error.950}" },
          "disabled": {
            "$value": "{button.color.fg.destructive.default}",
            "alpha": 0.4
          }
        }
      },
      "border": {
        "secondary": {
          "default": { "$value": "{theme.color.fg.link.default}" },
          "hover": { "$value": "{theme.color.fg.link.hover}" },
          "active": { "$value": "{theme.color.fg.link.active}" },
          "disabled": {
            "$value": "{button.color.border.secondary.default}",
            "alpha": 0.4
          }
        },
        "destructive": {
          "default": { "$value": "{button.color.fg.destructive.default}" },
          "hover": { "$value": "{button.color.fg.destructive.hover}" },
          "active": { "$value": "{button.color.fg.destructive.active}" },
          "disabled": {
            "$value": "{button.color.fg.destructive.default}",
            "alpha": 0.4
          }
        }
      }
    }
  },
  "datePicker": {
    "color": {
      "bg": {
        "selected": {
          "default": { "$value": "{primitive.color.primary.830}" }
