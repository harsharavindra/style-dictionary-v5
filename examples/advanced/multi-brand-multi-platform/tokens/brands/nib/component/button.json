          "hover": {
            "$value": "{primitive.color.primary.900}"
          },
          "active": {
            "$value": "{primitive.color.primary.850}"
          }
        }
      },
      "fg": {
        "primary": {
          "default": {
            "$value": "{primitive.color.primary.275}"
