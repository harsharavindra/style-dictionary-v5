          "hover": {
            "$value": "{primitive.color.primary.150}"
          },
          "active": {
            "$value": "{primitive.color.neutral.150}"
          }
        },
        "tertiary": {
          "default": {
            "$value": "{primitive.color.primary.275}"
          },
          "hover": {
            "$value": "{primitive.color.primary.150}"
          },
          "active": {
            "$value": "{primitive.color.neutral.150}"
          }
        }
      },
      "border": {
        "secondary": {
          "default": {
            "$value": "{primitive.color.primary.275}"
